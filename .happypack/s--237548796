var validate = function validate(values) {
	var errors = {};
	if (!values.groupName) {
		errors.groupName = '请填写分组名称';
	}
	if (!values.sort) {
		errors.sort = '请填写排序号';
	} else if (isNaN(+values.sort)) {
		errors.sort = '请输入数字';
	} else if (+values.sort <= 0) {
		errors.sort = '请输入正整数';
	} else if (values.sort.length >= 5) {
		errors.sort = '最多可输入4位数字';
	}

	if (!values.enable) {
		errors.enable = '请先选择是否启用';
	}

	return errors;
};

