var validate = function validate(values) {

	var errors = {};

	if (values.begin && values.begin.length != 10) {
		errors.begin = '请输入10位会员卡号';
	}
	if (isNaN(+values.begin)) {
		errors.begin = '请输入数字';
	}
	if (!values.begin) {
		errors.begin = '请填写起始号码';
	}

	if (values.end && values.end.length != 10) {
		errors.end = '请输入10位会员卡号';
	}
	if (isNaN(+values.end)) {
		errors.end = '请输入数字';
	}
	if (!values.end) {
		errors.end = '请填写终止号码';
	}

	return errors;
};

